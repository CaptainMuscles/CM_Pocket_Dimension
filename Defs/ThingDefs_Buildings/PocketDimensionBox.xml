<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="BuildingBase">
    <defName>CM_PocketDimensionEmpty</defName>
    <label>dimensional containment box</label>
	<thingClass>CM_PocketDimension.Building_PocketDimensionEmpty</thingClass>
    <description>An empty containment field for a pocket dimension.</description>
    <graphicData>	
      <texPath>Things/Building/SpacerProps/SpacerContainer/SpacerContainer</texPath>	
      <graphicClass>Graphic_Multi</graphicClass>
	  <drawSize>(1,1)</drawSize>
    </graphicData>
    <defaultPlacingRot>South</defaultPlacingRot>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <fillPercent>0.2</fillPercent>
    <statBases>
      <MaxHitPoints>500</MaxHitPoints>
      <WorkToBuild>500</WorkToBuild>
      <Mass>5</Mass>
      <Flammability>0</Flammability>
    </statBases>
	<building>
	</building>
    <size>(1,1)</size>
	<minifiedDef>MinifiedThing</minifiedDef>
	<thingCategories>
		<li>BuildingsSpecial</li>
	</thingCategories>
	<hasInteractionCell>True</hasInteractionCell>
	<interactionCellOffset>(0,0,1)</interactionCellOffset>
	<stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
	<costStuffCount>100</costStuffCount>
    <costList>
      <ComponentSpacer>2</ComponentSpacer>
    </costList>
    <!--<designationCategory>Misc</designationCategory>-->
    <uiIconScale>1</uiIconScale>
	<comps>
	  <li Class="CompProperties_Flickable"/>
	  <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>1000</basePowerConsumption>
      </li>
	  <li Class="CompProperties_Refuelable">
        <fuelConsumptionRate>0.0</fuelConsumptionRate>
        <fuelCapacity>1.0</fuelCapacity>
		<fuelLabel>Advanced components</fuelLabel>
        <fuelFilter>
          <thingDefs>
            <li>ComponentSpacer</li>
          </thingDefs>
        </fuelFilter>
        <showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
		<autoRefuelPercent>1.0</autoRefuelPercent>
		<drawOutOfFuelOverlay>false</drawOutOfFuelOverlay>
		<!--<initialFuelPercent>1.0</initialFuelPercent>-->
		<initialAllowAutoRefuel>false</initialAllowAutoRefuel>
      </li>
	</comps>
	<canOverlapZones>false</canOverlapZones>
	<tickerType>Normal</tickerType>
  </ThingDef>
  
  <ThingDef ParentName="BuildingBase">
    <defName>CM_PocketDimensionBox</defName>
    <label>pocket dimension</label>
	<thingClass>CM_PocketDimension.Building_PocketDimensionBox</thingClass>
    <description>A containment field for a pocket dimension.</description>
    <graphicData>
	  <texPath>Things/Building/SpacerProps/SpacerContainer/SpacerContainer</texPath>	
      <graphicClass>Graphic_Multi</graphicClass>
	  <drawSize>(1,1)</drawSize>
    </graphicData>
    <defaultPlacingRot>South</defaultPlacingRot>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <fillPercent>0.2</fillPercent>
    <statBases>
      <MaxHitPoints>500</MaxHitPoints>
      <WorkToBuild>500</WorkToBuild>
      <Mass>5</Mass>
      <Flammability>0</Flammability>
    </statBases>
	<building>
	</building>
    <size>(1,1)</size>
	<minifiedDef>MinifiedThing</minifiedDef>
	<thingCategories>
		<li>BuildingsSpecial</li>
	</thingCategories>
	<hasInteractionCell>True</hasInteractionCell>
	<interactionCellOffset>(0,0,1)</interactionCellOffset>
	<stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
	<costStuffCount>100</costStuffCount>
    <costList>
      <ComponentSpacer>2</ComponentSpacer>
    </costList>
    <designationCategory>Misc</designationCategory>
    <uiIconScale>1</uiIconScale>
	<comps>
		<!--<li Class="CompProperties_Flickable"/>-->
		<li Class="CompProperties_Transporter">
			<max1PerGroup>true</max1PerGroup>
			<massCapacity>1000</massCapacity>
			<restEffectiveness>0.8</restEffectiveness>
		</li>
		<li Class="CM_PocketDimension.CompProperties_PocketDimensionBatteryShare">
			<compClass>CM_PocketDimension.CompPocketDimensionBatteryShare</compClass>
			<storedEnergyMax>100.0</storedEnergyMax>
		</li>
		<li Class="CompProperties_Battery">
			<storedEnergyMax>100.0</storedEnergyMax>
			<efficiency>1.00</efficiency>
			<transmitsPower>true</transmitsPower>
		</li>
		<li Class="CompProperties_Refuelable">
			<fuelConsumptionRate>0.0</fuelConsumptionRate>
			<fuelCapacity>1.0</fuelCapacity>
			<fuelLabel>Advanced components</fuelLabel>
			<fuelFilter>
			  <thingDefs>
				<li>ComponentSpacer</li>
			  </thingDefs>
			</fuelFilter>
			<showAllowAutoRefuelToggle>false</showAllowAutoRefuelToggle>
			<autoRefuelPercent>1.0</autoRefuelPercent>
			<!--<initialFuelPercent>1.0</initialFuelPercent>-->
			<!--<initialAllowAutoRefuel>false</initialAllowAutoRefuel>-->
			<drawOutOfFuelOverlay>false</drawOutOfFuelOverlay>
		  </li>
	</comps>
	<inspectorTabs>
		<li>ITab_ContentsTransporter</li>
	</inspectorTabs>
	<canOverlapZones>false</canOverlapZones>
	<tickerType>Normal</tickerType>
	<tradeability>Sellable</tradeability>
	<tradeTags>
      <li>ExoticBuilding</li>
    </tradeTags>
	<researchPrerequisites>
      <li>CM_PocketDimension_Research_PocketDimension</li>
    </researchPrerequisites>
  </ThingDef>
  
